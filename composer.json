{
    "name": "evolabs/laravel-feature-flags",
    "description": "Add simple database feature flags",
    "keywords": [
        "evostudio",
        "evolabs",
        "laravel-feature-flags",
        "feature-flags"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dawid Herrlein",
            "email": "dawid.herrlein@evostudio.pl"
        }
    ],
    "version": "1.0",
    "require": {
        "php": "^8.1",
        "illuminate/cache": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/routing": "^9.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.9.x-dev",
        "laravel/pint": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Evolabs\\FeatureFlags\\": "src"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "pint": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Evolabs\\FeatureFlags\\FeatureFlagsServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}